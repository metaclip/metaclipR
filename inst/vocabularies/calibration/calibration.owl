    <!-- http://www.metaclip.org/calibration/calibration.owl#hasProbCharacter -->

    <owl:DatatypeProperty rdf:about="http://www.metaclip.org/calibration/calibration.owl#hasProbCharacter">
        <rdfs:range>
            <rdfs:Datatype>
