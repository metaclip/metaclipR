    <owl:ObjectProperty rdf:about="http://www.metaclip.org/calibration/calibration.owl#hadDeltaChange">
        <rdfs:subPropertyOf rdf:resource="http://www.metaclip.org/datasource/datasource.owl#hadTransformation"/>
        <rdfs:domain rdf:resource="http://www.metaclip.org/datasource/datasource.owl#Climatology"/>
        <rdfs:range rdf:resource="http://www.metaclip.org/datasource/datasource.owl#Climatology"/>
        <dc:description xml:lang="en">A delta change climatology is added to a climatological baseline to produce a future climate climatology</dc:description>
