    <!-- http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection -->

    <owl:Class rdf:about="http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/prov#Entity"/>
        <dc:description xml:lang="en">A map projection is a way to flatten a globe&apos;s surface into a plane in order to make a map. This requires a systematic transformation of the latitudes and longitudes of locations from the surface of the globe into locations on a plane</dc:description>
        <rdfs:comment xml:lang="en">This is a broad class. The different individual instances should be used in order to unequivocally define the map projection at hand</rdfs:comment>
        <rdfs:label>MapProjection</rdfs:label>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://spatialreference.org/</rdfs:seeAlso>
    </owl:Class>
    


