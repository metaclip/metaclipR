    <!-- http://www.metaclip.org/graphical_output/graphical_output.owl#epsg_code -->

    <owl:AnnotationProperty rdf:about="http://www.metaclip.org/graphical_output/graphical_output.owl#epsg_code">
        <dc:description xml:lang="en">A numeric code that identifies a specific spatial reference system</dc:description>
        <dc:title xml:lang="en">EPSG code</dc:title>
        <rdfs:comment xml:lang="en">EPSG Geodetic Parameter Dataset (also EPSG registry) is a public registry of spatial reference systems, Earth ellipsoids, coordinate transformations and related units of measurement. Each entity is assigned an EPSG code between 1024-32767,</rdfs:comment>
        <rdfs:seeAlso rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://en.wikipedia.org/wiki/EPSG_Geodetic_Parameter_Dataset</rdfs:seeAlso>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.metaclip.org/graphical_output/graphical_output.owl#isConformal -->

    <owl:AnnotationProperty rdf:about="http://www.metaclip.org/graphical_output/graphical_output.owl#isConformal">
        <dc:description xml:lang="en">Boolean. A conformal map projection is one in which every angle between two curves that cross each other on Earth (a sphere or an ellipsoid) is preserved in the image of the projection, i.e. the projection is a conformal map in the mathematical sense.</dc:description>
        <rdfs:label xml:lang="en">Conformal</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain rdf:resource="http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.metaclip.org/graphical_output/graphical_output.owl#isEqualArea -->

    <owl:AnnotationProperty rdf:about="http://www.metaclip.org/graphical_output/graphical_output.owl#isEqualArea">
        <dc:description xml:lang="en">Boolean. Equal-area maps preserve area measure, generally distorting shapes in order to do that.</dc:description>
        <rdfs:label xml:lang="en">Equal Area</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
        <rdfs:domain rdf:resource="http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.metaclip.org/graphical_output/graphical_output.owl#proj_string -->

    <owl:AnnotationProperty rdf:about="http://www.metaclip.org/graphical_output/graphical_output.owl#proj_string">
        <dc:description xml:lang="en">PROJ notation used to describe a certain Coordinate Reference System as a character string</dc:description>
        <ds:referenceURL rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://proj.org/</ds:referenceURL>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:domain rdf:resource="http://www.metaclip.org/graphical_output/graphical_output.owl#MapProjection"/>
    </owl:AnnotationProperty>
    


